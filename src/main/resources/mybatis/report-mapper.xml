<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.kh.onsoo.admin.reportboard">

	<resultMap type="ReportDto" id="ReportDtoMap">
		<result property="report_no" column="REPORT_NO"/>
		<result property="report_title" column="REPORT_TITLE"/>
		<result property="report_content" column="REPORT_CONTENT"/>
		<result property="report_date" column="REPORT_DATE"/>
		<result property="report_filename" column="REPORT_FILENAME"/>
		<result property="report_id" column="REPORT_ID"/>
		<result property="report_ided" column="REPORT_IDED"/>
	</resultMap>
	
	<select id="selectList" resultMap="ReportDtoMap">
		SELECT REPORT_NO, REPORT_TITLE, REPORT_DATE, REPORT_IDED FROM REPORT
	</select>
	<select id="selectOne" parameterType="int" resultType="ReportDto">
		SELECT REPORT_NO, REPORT_ID, REPORT_IDED, REPORT_TITLE, REPORT_CONTENT, REPORT_DATE
		FROM REPORT
		WHERE REPORT_NO = #{repot_no}
	</select>
	<update id="update" parameterType="ReportDto">
		UPDATE REPORT
		SET 
	</update>
	

	

</mapper>